<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="Odyssey.Views.AdditionalDevTools.JavascriptConsolePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Odyssey.Views.AdditionalDevTools"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:monaco="using:Monaco"
    mc:Ignorable="d"
    >

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Button Style="{ThemeResource AccentButtonStyle}" Click="ExecuteButton_Click"/>
        </Grid>
        <!--Monaco editor-->
    </Grid>
</Page>
